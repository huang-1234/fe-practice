{
  "files.associations": {
    "string": "cpp",
  },
  "editor.fontSize": 12,
  "[C_Cpp]": {
    "formatting": "vcFormat",
    "vcFormat.space.insertAfterSemicolon": true,
    "updateChannel": "Insiders",
    "vcFormat.newLine.closeBraceSameLine.emptyFunction": true,
    // "autocompleteAddParentheses": true,
    "vcFormat.space.betweenEmptyParameterListParentheses": true,
    "vcFormat.newLine.beforeOpenBrace.block": "sameLine",
    "vcFormat.newLine.beforeOpenBrace.function": "sameLine",
    "vcFormat.newLine.beforeOpenBrace.lambda": "sameLine",
    "vcFormat.newLine.beforeOpenBrace.namespace": "sameLine",
    "vcFormat.newLine.beforeOpenBrace.type": "sameLine",
    "clang_format_style": {
      "BasedOnStyle": "Chromium",
      // "BasedOnStyle": "Google",
      "IndentWidth": 2
    },
    "clang_format_fallbackStyle": "{ BasedOnStyle: Google, IndentWidth: 4, ColumnLimit: 0}"
    // "editor.defaultFormatter": "ms-vscode.cpptools"
  },
  "code-runner.executorMap": {
    "javascript": "node",
    "php": "C:\\php\\php.exe",
    "python": "python",
    "perl": "perl",
    "ruby": "C:\\Ruby23-x64\\bin\\ruby.exe",
    "go": "go run",
    "html": "\"C:\\Program Files (x86)\\Google\\Chrome\\Application\\chrome.exe\"",
    "java": "cd $dir && javac $fileName && java $fileNameWithoutExt",
    "c": "cd $dir && gcc $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt"
  },

  "[cpp]": {
    "editor.defaultFormatter": "ms-vscode.cpptools"
  },
  "C_Cpp.vcFormat.wrap.preserveBlocks": "allOneLineScopes",
  "clang-format.executable": "/absolute/path/to/clang-format",
  "[c]": {
    "editor.defaultFormatter": "ms-vscode.cpptools"
  },

  "editor.tokenColorCustomizations": {
    "[Visual Studio Dark]": {
      "comments": "#7e3648",
      "strings": "#b9b156",
      "functions": "#8ed3cd",
      "keywords": "#cc0cee",
      "variables": "#a9e7bc",
      "numbers": "#83024d83",
      "textMateRules": [
        {
          "name": "Comment",
          "scope": [
            "comment"
          ],
          "settings": {
            "foreground": "#728872",
            "fontStyle": ""
          }
        },
        {
          "name": "[VSCODE-CUSTOM] PHP Punctuation Variable Definition",
          "scope": "punctuation.definition.variable.php",
          "settings": {
            "foreground": "#4add1d"
          }
        },
        {
          "name": "String",
          "scope": [
            "String"
          ],
          "settings": {
            "foreground": "#4be5eb",
            "fontStyle": ""
          }
        },
        {
          "name": "variables",
          "scope": [
            "variables"
          ],
          "settings": {
            "foreground": "#bd9860",
            "fontStyle": ""
          }
        },
        {
          "name": "HTML:Tags",
          "scope": [
            "meta.tag",
            "punctuation.definition.tag.html",
            "punctuation.definition.tag.begin.html",
            "punctuation.definition.tag.end.html"
          ],
          "settings": {
            "foreground": "#b1a1a1",
            "fontStyle": ""
          }
        },
        {
          "name": "HTML:Tag Names",
          "scope": "entity.name.tag",
          "settings": {
            "foreground": "#8bbbb6",
            "fontStyle": ""
          }
        },
        {
          "name": "HTML: Attribute Names",
          "scope": [
            "meta.tag entity.other.attribute-name",
            "entity.other.attribute-name.html"
          ],
          "settings": {
            "fontStyle": "italic",
            "foreground": "#e67171"
          }
        },
        {
          "name": "Operator",
          "scope": "keyword.operator",
          "settings": {
            "foreground": "#e72907",
            "fontStyle": ""
          }
        }
      ]
    }
  },
  "powermode.enabled":true,
  "powermode.enableShake":false,
  "powermode.presets":"flames",
  "powermode":{
    "enabled":true,
      //样式
    // "presets":"fireworks",
    "presets":"flames",
    // 时间间隔
    "comboTimeout":10,
    // 抖动
    "enableShake":false,
    "backgroundMode":"mask"
  },
  // "workbench.colorTheme": "Visual Studio Dark",
  "workbench.colorCustomizations": {
    "[Quiet Light]": {
      "editor.foreground": "#332b2b",
      "editor.selectionHighlightBorder": "#94767c00",
      "editor.selectionHighlightBackground": "#ff000078",
      "editorIndentGuide.activeBackground": "#81868d",
      "editorBracketMatch.background": "#addb9f5e",
      "editorBracketMatch.border": "#ff0000",
      "tab.activeBackground": "#ad9cd4b2",
      "textLink.foreground": "#d6561ac4",
      "descriptionForeground": "#ff0000",
      "selection.background": "#b98cd693",
      "textBlockQuote.background": "#b89a9a",
      "textSeparator.foreground": "#316f8f"
    },
  },
  // "compile-hero.disable-compile-files-on-did-save-code": true,
  // "compile-hero": {
  //   "disable-compile-files-on-did-save-code": false, // 全局开关，false 为开启自动编译， 默认 true 为开启不自动编译
  //   "notification-toggle": false, // 关闭编译失败时候右下角的弹窗提醒
  //   "javascript-output-directory": "./out", // javascript 输出的目录
  //   "javascript-output-toggle": false, // 局部开关，true 为开启 javascript 自动编译，false 为不开启 javascript 自动编译
  //   "sass-output-directory": "./out", // sass 输出的目录
  //   "sass-output-toggle": true, // 局部开关，true 为开启 sass 自动编译，false 为不开启 sass 自动编译
  //   "ignore": ["src/test.js", "*/test.scss", "**/spec/*", "**/src/**/*"], // 禁止文件和文件夹自动格式化和编译
  //   "watch": ["sass/test.sass", "**/less/**/*"] // 监听对应文件或者文件夹，保存的时候触发编译，一般配合 Compile Hero: On 开关打开使用
  // },

  "editor":{
    "parameterHints":true,
    "quickSuggestions":{
      "other": true,
      "comments": true,
      "strings": true
    }
  },
  "files.trimTrailingWhitespace":true,
  "leetcode.endpoint": "leetcode-cn",
  "leetcode.workspaceFolder": "G:\\Study\\Code\\Web\\myNotes\\learnFrontTest\\Algorithm\\leetcode",
  "leetcode.defaultLanguage": "javascript",
}